#include <stdio.h>

int main() {
    int n = 4; // Set the value of n

    // Loop through rows
    for (int i = 1; i <= n; i++) {
        // Print numbers in each row
        for (int j = 1; j <= i; j++) {
            printf("%d ", j);
        }
        printf("\n"); // Move to the next line
    }

    return 0;
}
